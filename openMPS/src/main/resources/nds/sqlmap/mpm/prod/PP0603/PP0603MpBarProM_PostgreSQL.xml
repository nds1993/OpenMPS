	<select id="PP0603mpBarProMDAO.selectMpBarProMList_D" parameterClass="PP0603mpBarProMSerarchVO" resultClass="egovMap">
		SELECT a.pro_date as proDate,
		       a.pro_time as proTime,
		       b.plant_name as plantCode,
		       c.code_name as sumaeCode,
		       d.code_name as nongName,
		       f.code_name as proLcode,
		       e.pro_name as proCode,
		       a.box_weig as weight,
		       g.code_name as slaughterHouse,
		       a.manufacturer as manufacture,
		       h.code_name as saveQty,
		       a.shelf_life as shelfLife,
		       a.temperature,
		       a.bar_code as barCode,
		       a.bunch_no as hisNo
		  FROM openmps.mp_bar_pro_m a
		  LEFT JOIN openmps.tm_codexd c ON a.su_code = c.code_id
		                              AND a.corp_code = c.corp_code
		                              AND c.group_code = 'PP001' /*수매기관*/
		  LEFT JOIN openmps.tm_codexd d ON a.nong_code = d.code_id
		                              AND a.corp_code = d.corp_code
		                              AND d.group_code = 'PP010' /*농장명*/
		  LEFT JOIN openmps.tm_codexd g ON a.slaughterhouse = g.code_id
		                              AND a.corp_code = g.corp_code
		                              AND g.group_code = 'PP008', /*도축장*/
		 openmps.tm_platxm b, openmps.mp_item_master_m e
		  LEFT JOIN openmps.tm_codexd f ON e.large_code = f.code_id
		                              AND e.corp_code = f.corp_code
		                              AND f.group_code = 'MP006' /*품목*/
		  LEFT JOIN openmps.tm_codexd h ON e.frozen_type = h.code_id
		                              AND e.corp_code = h.corp_code
		                              AND h.group_code = 'MP008' /*냉장/냉동*/
		 WHERE a.corp_code = #corpCode#
		   AND a.pro_date BETWEEN #strtDate# AND #lastDate#
		   AND a.prdt_type IN ('1','2')
		   AND a.dele_yn = 'N'
		   AND a.corp_code = b.corp_code
		   AND a.plant_code = b.plant_code
		   AND a.corp_code = e.corp_code
		   AND a.pro_code = e.pro_code
		   AND a.plant_code IN (SELECT a.plant_code
		                          FROM openmps.tm_platxm a
		                         WHERE a.corp_code = #corpCode#
		                           AND a.grup_plant = #plantCode#)
		UNION ALL
		
		SELECT work_date AS proDate,
		       substr(a.print_code, 9, 2) || ':' || substr(a.print_code, 11, 2) || ':' || substr(a.print_code, 13, 2) AS proTime,
		       c.plant_name as plantCode,
		       '' AS sumaeCode,
		       '' AS nongName,
		       f.code_name as proLcode,
		       b.pro_name as proCode,
		       b.pro_weig as weight,
		       '' AS slaughterhouse,
		       '' AS manufacturer,
		       h.code_name as saveQty,
		       '' AS shelfLife,
		       '' AS temperature,
		       a.print_code as barCode,
		       a.bunch_no as hisNo
		  FROM openmps.mp_auto_total_m a, openmps.mp_item_master_m b
		  LEFT JOIN openmps.tm_codexd f ON b.large_code = f.code_id
		                              AND b.corp_code = f.corp_code
		                              AND f.group_code = 'MP006' /*품목*/
		  LEFT JOIN openmps.tm_codexd h ON b.frozen_type = h.code_id
		                              AND b.corp_code = h.corp_code
		                              AND h.group_code = 'MP008' /*냉장/냉동*/                              
		 , openmps.tm_platxm c
		 WHERE a.corp_code = #corpCode#
		   AND work_date BETWEEN #strtDate# AND #lastDate#
		   AND a.dele_yn = 'N'
		   AND a.corp_code = b.corp_code
		   AND a.pro_name = b.pro_name
		   AND a.corp_code = c.corp_code
		   AND a.plant_code = c.plant_code
		   AND a.plant_code IN (SELECT a.plant_code
		                          FROM openmps.tm_platxm a
		                         WHERE a.corp_code = #corpCode#
		                           AND a.grup_plant = #plantCode#)
         ORDER BY 1, 3, 6, 7 
	</select>	
	
