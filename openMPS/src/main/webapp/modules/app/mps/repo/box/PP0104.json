{
	"name": "세트등록",
	"module": "PP0104Renderer",
	"options": {
		"className": "contents_box",
		"order": [
			"queryBox",
			"resultBox"
		],
		"activation": {
			"defaultWidget": "",
			"method": "one-page"
		},
		"queryBox": {
			"module": "FormBox",
			"options": {
				"module": "FormBox",
				"className": "query_box form-inline",
				"order": [
					"setCode"
				],
				"custom_form": {
					"mode": "form",
					"template": null,
					"items": {
						"setCode": {
							"module":"text",
							"label": "세트제품",
							"selector": ".setCode",
							"required": false
						}
					},
					"data": {}
				}
			}
		},
		"resultBox": {
			"module": "FormBox",
			"options": {
				"className": "result_box",
				"order": [
					"setitem",
					"setdetail"
				],
				"activation": {
					"defaultWidget": "",
					"method": "one-page"
				},
				"custom_form": {
					"mode": "form",
					"template": "<div class=\"row\"><div class=\"col-xs-3 setitem_region\" ></div><div class=\"col-xs-9 setdetail_region\"></div></div>",
					"items": {
						"setitem": {
							"label": "setitem",
							"required": false
						},
						"setdetail": {
							"label": "setdetail",
							"required": false
						}
					},
					"data": {}
				},
				"setitem": {
					"module": "manifest!jqGrid4-1.0.0",
					"options": {
						"className": "setitem_box grid_box mt27",
						"gridId": "PP0104_grid_1",
						"gridParams": {
							"data": [],
							"colModel": [{
									"name": "proCode",
									"label": "세트코드",
									"width": 30,
									"align": "center",
									"editable": false
								},
								{
									"name": "proName",
									"label": "세트명",
									"align": "left",
									"width": 80,
									"editable": false
								}
							],
							"rowNum":"100000000",
							"width": "auto",
							"height": "570",
							"cellEdit": false,
							"shrinkToFit": true,
							"gridview": true,
							"datatype": "local",
							"viewrecords": true,
							"footerrow": false,
							"multiselect": false,
							"multiselectWidth": 20,
							"rownumbers": false,
							"pager": "#PP0104_grid_1_pager",
							"viewrecords": true,
							"recordpos": "left",
							"pgbuttons": false,
							"pginput": false,
							"cellsubmit": "clientArray"
						}
					}
				},
				"setdetail": {
					"module": "SubContainer",
					"options": {
						"header": {
							"title": {
								"className": "contnet_header_box no_label",
								"label": "관할조직"
							},
							"feature": [{
									"create": "행추가"
								}
							]
						},
						"content": {

							"module": "manifest!jqGrid4-1.0.0",
							"options": {
								"className": "setdetail_box grid_box",
								"gridId": "PP0104_grid_2",
								"gridParams": {
									"data": [],
									"colModel": [{
											"name": "proCode",
											"label": "<div class=\"editable_mark\"></div>제품코드",
											"width": 60,
											"align": "center",
											"editable": true,
											"classes": "editable codesearch",
											"editrules":{"required":true}
										},
										{
											"name": "proName",
											"label": "제품명",
											"align": "left",
											"width": 150,
											"editable": false
										},
										{
											"name": "setQty",
											"label": "<div class=\"editable_mark\"></div>세트수량",
											"align": "right",
											"width": 60,
											"editable": true,
											"sorttype":"integer","formatter":"integer",
											"classes": "editable",
											"editrules":{"number":true,"required":true},
											"editoptions": {"maxlength": 9}
										},
										{
											"name": "useYn",
											"label": "<div class=\"editable_mark\"></div>사용유무",
											"width": 60,
											"align": "center",
											"editable": true,
											"formatter": "select",
											"classes": "editable",
											"edittype":"select",
											"editoptions": {
												"value": "Y:사용;N:사용안함"
											}
										},
										{
											"name": "memo",
											"label": "<div class=\"editable_mark\"></div>비고",
											"align": "left",
											"width": 200,
											"editable": true,
											"classes": "editable"
										}
									],
									"rowNum":"100000000",
									"width": "auto",
									"height": "570",
									"cellEdit": true,
									"cellsubmit": "clientArray",
									"shrinkToFit": true,
									"gridview": true,
									"datatype": "local",
									"viewrecords": true,
									"footerrow": false,
									"multiselect": false,
									"multiselectWidth": 10,
									"rownumbers": false,
									"cellsubmit": "clientArray",
									"pager": "#PP0104_grid_2_pager",
									"viewrecords": true,
									"recordpos": "left",
									"pgbuttons": false,
									"pginput": false,
									"cellsubmit": "clientArray"

								}
							}
						}
					}
				}
			}
		}
	}
}
