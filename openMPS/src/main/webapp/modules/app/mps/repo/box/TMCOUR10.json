							"result": null
						}
					}
				},
                "deptCodeSearch": {
					"module": "manifest!CodeSearch-1.0.0",
					"options": {
						"label": "파트코드검색",
						"required": false,
						"codeType": "TMCOOS70",
						"params": {},
						"value": {
							"keyword": null,
							"result": null
						}
					}
				},
				"address": {
					"module": "manifest!address-1.3.0",
					"options": {
						"label": "주소찾기",
						"required": false,
						"template": "#zip_two_html"
					}
				}
            }
        },
        "queryBox": {
            "module": "FormBox",
            "options": {
                "module": "FormBox",
                "className": "query_box form-inline",
                "order": [
                    "searchUserName",
                    "searchUsdnCode",
                    "searchUseYn"
                ],
                "custom_form": {
                    "mode": "form",
                    "template": "<div class=\"form-group \"><label>사용자명</label><input type=\"text\" class=\"form-control searchUserName\" value=\"\" placeholder=\"사용자명 입력\"></div><div class=\"form-group \"><label>사용자구분</label><div class=\"select_design searchUsdnCode\"><span class=\"select_label\"></span><select class=\"form-control\"></select></div></div><div class=\"form-group \"><label>사용여부</label><div class=\"select_design searchUseYn\"><span class=\"select_label\"></span><select class=\"form-control\"></select></div></div>",
                    "items": {
                        "searchUserName": {
                            "module": "text",
                            "label": "사용자명",
                            "selector": ".searchUserName",
                            "required": false
