							}
                        },
                        "searchUseYn": {
                            "selector": ".searchUseYn",
                            "label": "사용여부",
                            "required": false,
                            "module": "combobox",
                            "value": "",
							"items": [{
									"label": "선택",
									"value": ""
								},
								{
									"label": "사용",
									"value": "Y"
								},
								{
									"label": "미사용",
									"value": "N"
								}
							]
                        }
                    },
                    "data": {}
                }
            }
