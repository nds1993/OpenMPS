							}, {
								"name": "salePrice",
								"label": "변경가격",
								"width": 80,
								"align": "right",
								"editable": false,
								"sorttype": "currency" , "formatter": "currency"

							}, {
								"name": "stndPrice",
								"label": "표준가격",
								"width": 80,
								"align": "right",
								"editable": false,
								"sorttype": "currency" , "formatter": "currency"
							}, {
								"name": "stndIncr",
								"label": "표준증감",
								"width": 80,
								"align": "right",
								"editable": false,
								"sorttype": "currency" , "formatter": "currency"
							}, {
								"name": "increase",
								"label": "<div class=\"editable_mark\"></div>증감",
								"width": 80,
								"align": "right",
								"editable": true,
								"edittype":"text",
								"sorttype": "currency" , "formatter": "currency",
								"classes":"editable",
								"editrules":{ "number":true}
							}, {
								"name": "increasePrice",
								"label": "증감차액",
								"width": 80,
								"align": "right",
								"editable": false,
								"sorttype": "currency" , "formatter": "currency",
								"classes":"color_minus"

							}, {
								"name": "discPer",
								"label": "할인율",
								"width": 80,
								"align": "right",
								"editable": false,
								"sorttype":"number","formatter":"number"
							}, {
								"name": "approYnName",
								"label": "확인",
								"width": 60,
								"align": "center",
								"editable": false
							},
			                {
			                  "name": "approYn",
			                  "label": "approYn",
			                  "width": 40,
			                  "align": "center",
			                  "hidden": true
			                }],
							"rowNum":"100000000",
							"width": "auto",
							"height": "572",
							"cellEdit": true,
							"shrinkToFit": true,
							"gridview": true,
							"datatype": "local",
							"footerrow": false,
							"multiselect": true,
							"multiselectWidth": 19,
							"rownumbers": false,
							"cellsubmit": "clientArray",
							"pager": "#SD0206_grid_1_pager",
							"viewrecords": true,
							"recordpos": "left",
							"pgbuttons": false,
							"pginput": false

						}
					}
				}
			}
		}
