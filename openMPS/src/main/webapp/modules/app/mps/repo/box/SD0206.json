{
	"name": "판매단가 승인",
	"module": "SD0206Renderer",
	"options": {
		"className": "contents_box",
		"order": ["queryBox", "resultBox"],
		"activation": {
			"defaultWidget": "",
			"method": "one-page"
		},
		"queryBox": {
			"module": "FormBox",
			"options": {
				"className": "query_box  form-inline",
				"order": ["strtDate", "lastDate", "status"],
				"activation": {
					"method": "one-page"
				},
				"custom_form": {
					"mode": "form",
					"name": "API Runner Form",
					"items": {
						"strtDate": {
							"module": "date",
							"selector": ".strtDate",
							"label": "strtDate",
							"required": false,
							"format": "YYYY-MM-DD",
							"useCurrent": true,
							"defaultDate": true
						},
						"lastDate": {
							"module": "date",
							"selector": ".lastDate",
							"label": "lastDate",
							"required": false,
							"format": "YYYY-MM-DD",
							"useCurrent": true,
							"defaultDate": true
						},
						"status": {
							"selector": ".status",
							"label": "확인상태",
							"required": false,
							"module": "combobox",
							"items": [
							{
								"label": "전체",
								"value": "0"
							},{
								"label": "대기",
								"value": "nappro"
							}, {
								"label": "승인",
								"value": "yappro"
							}]
						}
					},
					"data": {},
					"template": "<div class=\"form-group\"><label>시작일자</label><input type=\"text\" class=\"form-control form_type_date strtDate\" placeholder=\"\">~ <input type=\"text\" class=\"form-control form_type_date lastDate\" placeholder=\"\"></div><div class=\"form-group\"><label>확인상태</label><div class=\"select_design status\"><span class=\"select_label\"></span><select class=\"form-control\"></select></div>"
				}
			}
		},
		"resultBox": {
			"module": "SubContainer",
			"options": {
				"header": {
					"title": {
						"className": "contnet_header_box no_label",
						"label": "판매단가승인"
					},
					"feature": [{"confirm": "단가승인"},
						{"reject": "승인취소"}
					]
				},
				"content": {
					"module": "manifest!jqGrid4-1.0.0",
					"options": {
						"className": "grid_box",
						"gridId": "SD0206_grid_1",
						"gridParams": {
							"data": [],
							"colModel": [{
								"name": "custCode",
								"label": "거래처",
								"width": 170,
								"align": "left",
								"editable": false,
								"hidden":true
							}, {
								"name": "custName",
								"label": "거래처",
								"width": 170,
								"align": "left",
								"editable": false
							}, {
								"name": "strtDate",
								"label": "시작일자",
								"width": 90,
								"align": "center",
								"editable": false,
								"sorttype": "date" , "formatter": "dateFormatter",
				                "unformat": "dateUnformatter",
				                "formatoptions": {
				                    "srcformat": "YYYYMMDD",
				                    "newformat": "YYYY-MM-DD"
				                }
							}, {
								"name": "lastDate",
								"label": "종료일자",
								"width": 90,
								"align": "center",
								"editable": false,
								"sorttype": "date" , "formatter": "dateFormatter",
				                "unformat": "dateUnformatter",
				                "formatoptions": {
				                    "srcformat": "YYYYMMDD",
				                    "newformat": "YYYY-MM-DD"
				                }

							}, {
								"name": "largeCodename",
